#ifndef UART_H_
#define UART_H_

#include "stm32f4xx.h"

#define GPIOA_EN      (1U<<0)
#define GPIOA_AF_PIN4 (1U<<4)
#define GPIOA_AF_PIN5 (1U<<5)

#define GPIOA_AFR_PIN8  (1U<<8)
#define GPIOA_AFR_PIN9  (1U<<9)
#define GPIOA_AFR_PIN10 (1U<<10)
#define GPIOA_AFR_PIN11 (1U<<11)
 void uart_write();
 void uart_read();
 void uart_enable();
 void uart_disable();
 void uart_init();

#endif /* UART_H_ */
