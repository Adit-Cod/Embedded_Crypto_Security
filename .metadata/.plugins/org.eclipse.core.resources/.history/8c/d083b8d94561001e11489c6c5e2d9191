#ifndef BSP_H_
#define BSP_H_

#include "stm32f4xx.h"

void led_init(void);
void led_on(void);
void led_off(void);


#define GPIOAEN (1U<<0)
#define PIN5    (1U<<5)
#define LED_PIN PIN5
#define GPIO_ENABLE  1U
#define GPIO_DISABLE 0U

#endif /* BSP_H_ */
